{% extends '::base.html.twig' %}

{% block body %}
    {% for page in pages %}
        <div clas="page">
            <h1>{{ page.title }}</h1>
            <p>{{ page.content|slice(0,25) }}...<a href="{{ path('custom_green_page_display', {'id': page.id}) }}">read more</a></p>
        </div>
    {% endfor %}
{% endblock %}